package lambda1.in;

interface College{
	void enroll();
}
class B implements College{

	@Override
	public void enroll() {
		// TODO Auto-generated method stub
		System.out.println("This is college");
		
	}
	
}

public class Student {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		B clgB = () -> clgB.enroll();
		
		

	}

}
