package lambda1.in;

interface College{
	void enroll();
}
interface MyAdd {
	 int mdd(int x, int y) {
		return x + y;
	}
	
	
}

public class Student {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		College clg1 = () -> System.out.println("Hey We are using Lambda Expression");
		clg1.enroll();
		MyAdd poj = (p, q) -> retu (p + q);
		poj.mdd(5, 5);
	}
	
}
			
	
